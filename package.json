{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "web": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage",
    "lint": "tsc && tslint --fix -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-art": "16.6.3",
    "react-dom": "16.6.3",
    "react-native": "0.57.5",
    "react-native-web": "0.9.8",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/react": "16.7.6",
    "@types/react-native": "0.57.10",
    "@types/react-test-renderer": "16.0.3",
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "prettier": "1.15.2",
    "react-test-renderer": "16.6.3",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "typescript": "3.1.6"
  },
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.(jsx?)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.jest.json"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
