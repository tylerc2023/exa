{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://ethanneff.github.io/example",
  "scripts": {
    "ios": "                echo 'develop on ios';                react-native run-ios",
    "android": "            echo 'develop on android';            react-native run-android",
    "web": "                echo 'develop on web';                react-scripts start",
    "shake": "              echo 'shake android device';          adb shell input keyevent 82",
    "reload": "             echo 'reload android device';         adb shell input text 'RR'",
    "build": "              echo 'build production web';          react-scripts build",
    "start": "              echo 'run code watcher';              react-native start --reset-cache",
    "debug": "              echo 'run redux debugger';            open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "lint": "               echo 'run code linter';               eslint 'src/**/*.[jt]s?(x)' --fix && tsc",
    "test": "               echo 'run unit tests';                jest -u --forceExit --coverage --detectOpenHandles",
    "watch": "              echo 'run unit tests reloader';       npm run test -- --watchAll",
    "reset": "              echo 'clear react native cache';      watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf ./android/build; rm -rf ./android/.gradle; rm -rf ./android/app/build; rm -rf ~/Library/Developer/Xcode/DerivedData; rm -rf /tmp/haste-map-react-native-packager-*; rm -rf ./ios/build; npx jetify; cd android; ./gradlew clean; cd ..; react-native start --reset-cache",
    "semantic-release": "   echo 'run continuous integration';    semantic-release",
    "travis-deploy-once": " echo 'prevent multiple deploys';      travis-deploy-once"
  },
  "dependencies": {
    "@mdi/js": "4.5.95",
    "@mdi/react": "1.2.1",
    "@react-native-community/async-storage": "1.6.2",
    "@react-native-community/netinfo": "4.6.0",
    "@react-native-firebase/analytics": "6.0.3",
    "@react-native-firebase/app": "6.0.3",
    "@react-native-firebase/auth": "6.0.3",
    "@react-native-firebase/crashlytics": "6.0.3",
    "@react-native-firebase/firestore": "6.0.3",
    "@react-native-firebase/remote-config": "6.0.3",
    "appcenter": "2.5.0",
    "appcenter-analytics": "2.5.0",
    "appcenter-crashes": "2.5.0",
    "axios": "0.19.0",
    "dayjs": "1.8.17",
    "npm-check-updates": "3.2.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "0.61.4",
    "react-native-device-info": "5.2.1",
    "react-native-keyboard-aware-scroll-view": "0.9.1",
    "react-native-vector-icons": "6.6.0",
    "react-native-web": "0.11.7",
    "react-redux": "7.1.3",
    "react-scripts": "3.2.0",
    "redux": "4.0.4",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "typesafe-actions": "5.1.0",
    "utility-types": "3.9.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/runtime": "7.7.2",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@react-native-community/eslint-config": "0.0.5",
    "@semantic-release/changelog": "3.0.5",
    "@semantic-release/commit-analyzer": "6.3.2",
    "@semantic-release/git": "7.0.18",
    "@semantic-release/github": "5.5.5",
    "@semantic-release/release-notes-generator": "7.3.2",
    "@types/axios": "0.14.0",
    "@types/jest": "24.0.22",
    "@types/react": "16.9.11",
    "@types/react-native": "0.60.22",
    "@types/react-native-vector-icons": "6.4.4",
    "@types/react-redux": "7.1.5",
    "@types/react-test-renderer": "16.9.1",
    "@types/uuid": "3.4.6",
    "@typescript-eslint/parser": "2.6.1",
    "babel-jest": "24.9.0",
    "codecov": "3.6.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react-hooks": "2.2.0",
    "husky": "3.0.9",
    "metro-react-native-babel-preset": "0.57.0",
    "react-test-renderer": "16.11.0",
    "semantic-release": "15.13.30",
    "travis-deploy-once": "5.0.11",
    "typescript": "3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md",
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/src/utils/TestMocks/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "window": {}
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
