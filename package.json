{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://ethanneff.github.io/example",
  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "web": "react-scripts start",
    "shake": "adb shell input keyevent 82",
    "reload": "adb shell input text 'RR'",
    "build": "react-scripts build",
    "start": "react-native start --reset-cache",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "lint": "tsc && eslint '**/*.[jt]s?(x)' --cache --fix",
    "test": "jest -u --forceExit --coverage --detectOpenHandles",
    "watch": "jest -u --forceExit --detectOpenHandles --watchAll",
    "reset": "watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf ./android/build; rm -rf ./android/.gradle; rm -rf ./android/app/build; rm -rf ~/Library/Developer/Xcode/DerivedData; rm -rf /tmp/haste-map-react-native-packager-*; rm -rf ./ios/build; jest --clearCache; cd ios; pod install --repo-update; cd ..; npx jetify; cd android; ./gradlew clean; cd ..; react-native start --reset-cache",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "ncu": "ncu -u && yarn && cd ios && pod install",
    "visualize-size": "react-native-bundle-visualizer",
    "deploy-ios-core": "fastlane ios core",
    "deploy-ios-progress": "fastlane ios progress",
    "deploy-ios-journal": "fastlane ios journal",
    "deploy-android-core": "fastlane android core",
    "deploy-android-progress": "fastlane android progress",
    "deploy-android-journal": "fastlane android journal",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@mdi/js": "5.7.55",
    "@mdi/react": "1.4.0",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.7",
    "@react-native-community/slider": "3.0.3",
    "@react-native-firebase/analytics": "7.6.8",
    "@react-native-firebase/app": "8.4.6",
    "@react-native-firebase/auth": "9.3.1",
    "@react-native-firebase/crashlytics": "8.4.11",
    "@react-native-firebase/firestore": "7.8.7",
    "@react-native-firebase/remote-config": "9.0.11",
    "@react-navigation/bottom-tabs": "5.9.2",
    "@react-navigation/native": "5.7.6",
    "@react-navigation/stack": "5.9.3",
    "axios": "0.20.0",
    "d3-shape": "2.0.0",
    "date-fns": "2.16.1",
    "dayjs": "1.9.3",
    "howler": "2.2.0",
    "npm-check-updates": "9.1.2",
    "patch-package": "6.2.2",
    "postinstall-postinstall": "2.1.0",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-native": "0.63.3",
    "react-native-device-info": "7.0.2",
    "react-native-gesture-handler": "1.8.0",
    "react-native-get-random-values": "1.5.0",
    "react-native-linear-gradient": "2.5.6",
    "react-native-rate": "1.2.4",
    "react-native-reanimated": "1.13.1",
    "react-native-safe-area-context": "3.1.8",
    "react-native-screens": "2.11.0",
    "react-native-sensors": "7.0.1",
    "react-native-sound": "0.11.0",
    "react-native-svg": "12.1.0",
    "react-native-vector-icons": "7.1.0",
    "react-native-web": "0.14.3",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.4",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "typesafe-actions": "5.1.0",
    "utility-types": "3.10.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/runtime": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.1.1",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/axios": "0.14.0",
    "@types/d3-shape": "2.0.0",
    "@types/howler": "2.2.1",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.53",
    "@types/react-native": "0.63.27",
    "@types/react-native-keyboard-spacer": "0.4.1",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.3",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@welldone-software/why-did-you-render": "4.3.2",
    "babel-plugin-transform-remove-console": "6.9.4",
    "codecov": "3.8.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "metro-react-native-babel-preset": "0.63.0",
    "prettier": "2.1.2",
    "react-native-bundle-visualizer": "2.2.1",
    "react-native-dev-menu": "4.0.2",
    "react-native-highlight-updates": "1.2.0",
    "react-native-keyboard-spacer": "0.4.1",
    "react-test-renderer": "17.0.0",
    "semantic-release": "17.2.1",
    "travis-deploy-once": "5.0.11",
    "typescript": "4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md",
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      ".+\\.(bmp|gif|jpg|jpeg|mp4|png|psd|svg|webp|ttf|otf|m4v|mov|mp4|mpeg|mpg|webm|aac|aiff|caf|m4a|mp3|wav|html|pdf|obj)$": "<rootDir>/src/mocks/Files/index.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-community|@react-navigation|react-native-linear-gradient)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/src/mocks/Setup/index.ts"
    ],
    "globals": {
      "window": {}
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
