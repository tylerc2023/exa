{
  "name": "example",
  "version": "0.0.0",
  "private": true,
  "homepage": "http://ethanneff.github.io/example",
  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "web": "react-scripts start",
    "shake": "adb shell input keyevent 82",
    "reload": "adb shell input text 'RR'",
    "build": "react-scripts build",
    "start": "react-native start --reset-cache",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "lint": "tsc && eslint '**/*.[jt]s?(x)' --cache",
    "test": "jest -u --forceExit --coverage --detectOpenHandles",
    "watch": "jest -u --forceExit --detectOpenHandles --watchAll",
    "reset": "watchman watch-del-all; rm -rf /tmp/metro-bundler-cache-*; rm -rf ./android/build; rm -rf ./android/.gradle; rm -rf ./android/app/build; rm -rf ~/Library/Developer/Xcode/DerivedData; rm -rf /tmp/haste-map-react-native-packager-*; rm -rf ./ios/build; jest --clearCache; cd ios; pod install --repo-update; cd ..; npx jetify; cd android; ./gradlew clean; cd ..; react-native start --reset-cache",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "ncu": "ncu -u && yarn && yarn lint"
  },
  "dependencies": {
    "@mdi/js": "5.2.45",
    "@mdi/react": "1.4.0",
    "@react-native-community/async-storage": "1.10.1",
    "@react-native-community/netinfo": "5.9.0",
    "@react-native-community/slider": "2.0.9",
    "@react-native-firebase/analytics": "7.0.1",
    "@react-native-firebase/app": "7.0.1",
    "@react-native-firebase/auth": "7.0.2",
    "@react-native-firebase/crashlytics": "7.0.1",
    "@react-native-firebase/firestore": "7.0.1",
    "@react-native-firebase/remote-config": "7.0.1",
    "appcenter": "3.0.2",
    "appcenter-analytics": "3.0.2",
    "appcenter-crashes": "3.0.2",
    "axios": "0.19.2",
    "date-fns": "2.13.0",
    "dayjs": "1.8.27",
    "howler": "2.2.0",
    "npm-check-updates": "6.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.62.2",
    "react-native-device-info": "5.5.7",
    "react-native-get-random-values": "1.4.0",
    "react-native-keyboard-aware-scroll-view": "0.9.1",
    "react-native-rate": "1.2.1",
    "react-native-sensors": "5.4.1",
    "react-native-sound": "0.11.0",
    "react-native-vector-icons": "6.6.0",
    "react-native-web": "0.12.2",
    "react-redux": "7.2.0",
    "react-scripts": "3.4.1",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "typesafe-actions": "5.1.0",
    "utility-types": "3.10.0",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/runtime": "7.9.6",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.6",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/axios": "0.14.0",
    "@types/howler": "2.1.2",
    "@types/jest": "25.2.2",
    "@types/react": "16.9.35",
    "@types/react-native": "0.62.10",
    "@types/react-native-vector-icons": "6.4.5",
    "@types/react-redux": "7.1.8",
    "@types/react-test-renderer": "16.9.2",
    "@types/uuid": "7.0.3",
    "@typescript-eslint/parser": "2.33.0",
    "codecov": "3.7.0",
    "eslint": "7.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "semantic-release": "17.0.7",
    "travis-deploy-once": "5.0.11",
    "typescript": "3.9.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md",
          "changelogTitle": "# Changelog"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(pdf|mp3)$": "<rootDir>/src/utils/TestFiles/index.ts"
    },
    "setupFiles": [
      "<rootDir>/src/utils/TestMocks/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "window": {}
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
